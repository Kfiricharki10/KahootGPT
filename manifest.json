{
  "name": "KahootGPT",
  "version": "1.1.0",
  "manifest_version": 3,
  "description": "Kahoot & ChatGPT: Dominate in a Kahoot game",
  "author": "Marsss' Dev Team",
  "permissions": [
        "scripting",
        "activeTab",
        "storage",
        "tabs"
  ],
  "host_permissions": ["https://kahoot.it/*"],
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
        {
            "matches": ["https://kahoot.it/*"],
            "js": ["ExtPay.js"],
            "run_at": "document_start"
        },
        {
            "matches": ["http://*/", "https://*/"],
            "js": ["ExtPay.js"]
        }
    ]
}